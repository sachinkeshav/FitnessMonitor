<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>FitnessMonitor</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>NewActivity</servlet-name>
		<servlet-class>org.mustard.fitnessmonitor.servlets.NewActivity</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NewActivity</servlet-name>
		<url-pattern>/newActivity</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ShowHistory</servlet-name>
		<servlet-class>org.mustard.fitnessmonitor.servlets.ShowHistory</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShowHistory</servlet-name>
		<url-pattern>/showHistory</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddActivity</servlet-name>
		<servlet-class>org.mustard.fitnessmonitor.servlets.AddActivity</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddActivity</servlet-name>
		<url-pattern>/addActivity</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MyProfile</servlet-name>
		<servlet-class>org.mustard.fitnessmonitor.servlets.MyProfile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyProfile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
</web-app>